
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CF</title>
</head>
<body>
    <div>
        @for (int i = 1; i < 10; i++)
        {
            for (int j = 1; j < i+1; j++)
            {
               
               @(j+"*"+i+"="+(i*j))<a>&nbsp;&nbsp;&nbsp;</a>
            }
            <br />
        }
    </div>
</body>
</html>
